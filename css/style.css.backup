/* Featured Dishes Section (No Bootstrap) */
.featured-section {
  padding: 80px 0;
  background: linear-gradient(180deg, var(--bg-1) 0%, var(--bg-0) 100%);
}

.section-container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 20px;
}

.section-heading {
  text-align: center;
  font-size: 42px;
  font-weight: 800;
  color: var(--text);
  margin-bottom: 50px;
  position: relative;
}

.section-heading::after {
  content: '';
  display: block;
  width: 80px;
  height: 4px;
  background: var(--accent);
  margin: 20px auto 0;
  border-radius: 2px;
}

.dishes-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
  gap: 30px;
  margin-top: 40px;
}

.dish-card {
  background: var(--surface);
  border: var(--card-border);
  border-radius: 12px;
  overflow: hidden;
  transition: all 0.3s ease;
  cursor: pointer;
}

.dish-card:hover {
  transform: translateY(-8px);
  box-shadow: var(--shadow);
  border-color: var(--accent);
}

.dish-image {
  width: 100%;
  height: 240px;
  overflow: hidden;
}

.dish-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.4s ease;
}

.dish-card:hover .dish-image img {
  transform: scale(1.1);
}

.dish-info {
  padding: 24px;
}

.dish-info h3 {
  font-size: 22px;
  font-weight: 700;
  color: var(--text);
  margin-bottom: 12px;
}

.dish-info p {
  font-size: 15px;
  line-height: 1.6;
  color: var(--text-muted);
  margin-bottom: 16px;
  min-height: 72px;
}

.dish-footer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16px;
}

.price {
  font-size: 24px;
  font-weight: 800;
  color: var(--accent);
}

.rating {
  color: var(--accent-2);
  font-size: 16px;
}

.order-btn {
  display: block;
  width: 100%;
  padding: 12px;
  background: var(--accent);
  color: var(--bg-0);
  text-align: center;
  text-decoration: none;
  font-weight: 700;
  border-radius: 6px;
  transition: all 0.3s ease;
}

.order-btn:hover {
  background: var(--accent-pressed);
  transform: translateY(-2px);
  box-shadow: 0 6px 16px rgba(244, 195, 98, 0.35);
}

/* Menu Section Styles */
.menu-section {
  padding: 80px 0;
  background: linear-gradient(135deg, var(--bg-1) 0%, var(--bg-0) 100%);
}

.menu-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 30px;
  margin-top: 40px;
}

.menu-card {
  background: rgba(255, 255, 255, 0.06);
  border: 1px solid rgba(244, 195, 98, 0.3);
  border-radius: 16px;
  padding: 40px 30px;
  text-align: center;
  transition: all 0.4s ease;
  text-decoration: none;
  color: var(--text);
  display: block;
}

.menu-card:hover {
  background: rgba(244, 195, 98, 0.1);
  border-color: var(--accent);
  transform: translateY(-8px);
  box-shadow: 0 12px 40px rgba(244, 195, 98, 0.25);
  text-decoration: none;
  color: var(--text);
}

.menu-icon {
  margin-bottom: 20px;
  transition: transform 0.3s ease;
}

.menu-card:hover .menu-icon {
  transform: scale(1.1) rotate(5deg);
}

.menu-icon svg {
  width: 90px;
  height: 90px;
  filter: drop-shadow(0 4px 12px rgba(244, 195, 98, 0.3));
}

.menu-card h3 {
  color: var(--accent);
  font-size: 1.5rem;
  margin-bottom: 12px;
  font-weight: 600;
}

.menu-card p {
  color: var(--text-muted);
  font-size: 0.95rem;
  line-height: 1.6;
  margin: 0;
}

/* About Us Section (No Bootstrap) */
.about-section {
  padding: 80px 0;
  background: linear-gradient(180deg, var(--bg-0) 0%, var(--bg-1) 100%);
}

.about-content {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 50px;
  align-items: center;
  margin-top: 40px;
}

.about-image {
  width: 100%;
  height: 500px;
  overflow: hidden;
  border-radius: 12px;
  box-shadow: var(--shadow);
}

.about-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
  transition: transform 0.4s ease;
}

.about-image:hover img {
  transform: scale(1.05);
}

.about-text {
  color: var(--text-muted);
}

.about-text p {
  font-size: 16px;
  line-height: 1.8;
  margin-bottom: 20px;
  text-align: justify;
}

.about-text .order-btn {
  display: inline-block;
  width: auto;
  padding: 14px 32px;
  margin-top: 10px;
}

/* Testimonials Section */
.testimonials {
  padding: 80px 0;
  background: var(--bg-0);
}

.customer {
  text-align: center;
  font-size: 42px;
  font-weight: 800;
  color: var(--text);
  margin-bottom: 50px;
}

.customer::after {
  content: '';
  display: block;
  width: 80px;
  height: 4px;
  background: var(--accent);
  margin: 20px auto 0;
  border-radius: 2px;
}

.testimonials-container {
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 20px;
  overflow: hidden;
}

.testimonials-wrapper {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 30px;
}

.testimonial {
  background: var(--surface);
  border: var(--card-border);
  border-radius: 12px;
  padding: 30px;
  text-align: center;
  transition: all 0.3s ease;
}

.testimonial:hover {
  transform: translateY(-5px);
  box-shadow: var(--shadow);
  border-color: var(--accent);
}

.customer-photo {
  width: 100px;
  height: 100px;
  border-radius: 50%;
  object-fit: cover;
  margin: 0 auto 20px;
  border: 3px solid var(--accent);
  display: block;
}

.testimonial-info h3 {
  font-size: 20px;
  font-weight: 700;
  color: var(--text);
  margin-bottom: 10px;
}

.testimonial-info .rating {
  color: var(--accent-2);
  font-size: 18px;
  margin-bottom: 15px;
}

.testimonial-info p {
  font-size: 15px;
  line-height: 1.6;
  color: var(--text-muted);
  text-align: justify;
}

/* Generic image helper - prevent oversized images */
.img-fluid, .img-fluid-1 {
  max-width: 100%;
  height: auto;
  display: block;
}

@media (max-width: 992px) {
  .about-content {
    grid-template-columns: 1fr;
    gap: 30px;
  }

  .about-image {
    height: 400px;
  }

  .menu-grid {
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
