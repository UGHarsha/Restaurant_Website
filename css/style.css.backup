/* Style for the products section */
.products {
  padding: 20px;
  background-color: #f7f7f7;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.products .box-container {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 20px;
  width: 30%;
  max-width: 500px;
}

.products .box {
  background-color: #fff;
  border-radius: 10px;
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
  overflow: hidden;
  padding: 10px;
  text-align:center;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.products .box:hover {
  transform: translateY(-10px);
  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.2);
}

.products .box img {
  max-width: 100%;
  height: auto;
  margin-bottom: 5px;
  border-radius: 10px;
}

.products .box .name {
  font-size: 20px;
  font-weight: bolder;
  color: #333;
  margin-bottom: 10px;
  
}

.products .box .description {
  font-size: 16px;
  color: #666;
  font-weight: bold;
  margin-bottom: 15px;
  line-height: 1.5;
  height: 50px;
  overflow: hidden;
  text-overflow: ellipsis;
}

.products .box .flex {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 10px;
}

.products .box .price {
